<h1>Students</h1>

<form action="/students" method="get">
  <input type="text" placeholder="Search by student name" name="search">
  <input type="submit" value="Search">
</form>

<ul>
  <% @students.each do |student| %>
    <%= render partial: "student", locals: { student: student} %>
  <% end %>
</ul>